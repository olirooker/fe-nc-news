v>\n                    <label>\n                        <select defaultValue=\"\" onChange={(event) => { navigate(event.target.value) }}>\n                            <option key=\"topic-placeholder\" disabled={true} value=\"\">Select a topic</option>\n                            <option key=\"all-topics\" value=\"/\">all topics</option>\n                            {this.state.topics.map(topic => {\n                                return (\n                                    <option key={topic.slug} value={`/${topic.slug}/articles`}>{topic.slug}</option>\n                                )\n                            })}\n                        </select>\n                    </label>\n                </StyledNav>\n                <StyledSignIn>\n                    {username ? (\n                        <StyledUserContainer>\n                            <p>Hello, <Link to={`/users/${username}/articles`}>{username}</Link></p>\n                            <StyledAvatar src={avatar_url} alt=\"Your profile avatar\" />\n                        </StyledUserContainer>\n                    ) : (\n                            <Link to=\"/\"><p>Sign-in</p></Link>\n                        )}\n                </StyledSignIn>\n            </StyledHeader>\n        );\n    }\n}\n\nexport default Header;","/Users/olirooker/Documents/Northcoders/Frontend/fe-nc-news/nc-news-app/src/components/ArticlesList.js",[],"/Users/olirooker/Documents/Northcoders/Frontend/fe-nc-news/nc-news-app/src/components/TopicsList.js",[],"/Users/olirooker/Documents/Northcoders/Frontend/fe-nc-news/nc-news-app/src/components/UsersList.js",[],"/Users/olirooker/Documents/Northcoders/Frontend/fe-nc-news/nc-news-app/src/components/ArticleCard.js",[],"/Users/olirooker/Documents/Northcoders/Frontend/fe-nc-news/nc-news-app/src/components/TopicCard.js",[],"/Users/olirooker/Documents/Northcoders/Frontend/fe-nc-news/nc-news-app/src/api.js",[],"/Users/olirooker/Documents/Northcoders/Frontend/fe-nc-news/nc-news-app/s